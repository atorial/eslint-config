{
  "name": "@atoria/eslint-config",
  "version": "0.0.0",
  "description": "eslint configuration manager for all projects at @toria.africa",
  "main": ".output/index.js",
  "scripts": {
    "build": "tsc ",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "uno":"pnpm release --first-release",

    "prepare": "husky install"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-plugin",
    "atoria",
    "@atoria",
    "@webile",
    "code-lint"
  ],
  "files": [
    "./output/index.js",
    "README.md",
    "package.json"
  ],
  "publishConfig": {},
  "homepage": "https://libs.atoria.africa/eslint-config",
  "author": {
    "name": "Mitch Chanza ",
    "email": "hello@mitch.guru",
    "url": "https://mitch.guru"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.4",
    "husky": "^8.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  }
}
